.","ProductTitle":"Divinity: Original Sin - Enhanced Edition","Markets":["RU","DE","PT","NZ","AU","JP","KR","CN","TW","CA","MX","US","BR","GB","FR","IT","ES","AT","IE","HK","IN","SA","SG","ZA","AE","TR","CH","SE","NO","FI","DK","NL","BE","IL","CZ","PL","HU","SK","GR","AR","CL","CO","Neutral"]}],"MarketProperties":[{"ContentRatings":[{"RatingDescriptors":["ESRB:BloGor","ESRB:Vio"],"RatingDisclaimers":["ESRB:EsrOrn"],"RatingId":"ESRB:M","RatingSystem":"ESRB"}],"OriginalReleaseDate":"2015-10-27T00:00:00.0000000Z","UsageData":[{"AggregateTimeSpan":"7Days","AverageRating":4.3,"PlayCount":0,"RatingCount":6,"RentalCount":"0","TrialCount":"0","PurchaseCount":"0"},{"AggregateTimeSpan":"30Days","AverageRating":4.5,"PlayCount":0,"RatingCount":8,"RentalCount":"0","TrialCount":"0","PurchaseCount":"0"},{"AggregateTimeSpan":"AllTime","AverageRating":4.4,"PlayCount":0,"RatingCount":1905,"RentalCount":"0","TrialCount":"0","PurchaseCount":"0"}],"RelatedProducts":[{"RelatedProductId":"BVJ33GX66821","RelationshipType":"Parent"},{"RelatedProductId":"CFQ7TTC0K5DJ","RelationshipType":"Parent"},{"RelatedProductId":"BX1R6KDQ5P2R","RelationshipType":"Parent"},{"RelatedProductId":"BS2B3PRPQW3H","RelationshipType":"Parent"},{"RelatedProductId":"C3WRRX73F0VX","RelationshipType":"Parent"},{"RelatedProductId":"C3WRRX73F0VX","RelationshipType":"Extends"},{"RelatedProductId":"BVJ33GX66821","RelationshipType":"SellableBy"},{"RelatedProductId":"CFQ7TTC0K5DJ","RelationshipType":"SellableBy"},{"RelatedProductId":"BX1R6KDQ5P2R","RelationshipType":"SellableBy"},{"RelatedProductId":"BS2B3PRPQW3H","RelationshipType":"SellableBy"},{"RelatedProductId":"C3WRRX73F0VX","RelationshipType":"SellableBy"}],"Markets":["US"]}],"ProductASchema":"Product;3","ProductBSchema":"ProductGame;1","ProductId":"BPNSJSZCRPPF","Properties":{"Attributes":[{"Maximum":2,"Minimum":2,"Name":"OnlineMultiplayerWithGold"},{"Name":"CoopSupportLocal"},{"Name":"CoopSupportOnline"},{"Maximum":2,"Name":"LocalMultiplayer"}],"IsDemo":false,"Xbo